# ByteRoute (Docker Compose)
# Copy this file to `.env` in the repo root.

# Required: used by the backend container entrypoint to download GeoLite2 databases.
MAXMIND_USER_ID=
MAXMIND_API_KEY=

# Optional: change the exposed port (defaults to 8080).
TRAEFIK_PORT=8080

# Optional: demo mode (generates mock traffic events).
DEMO_MODE=false
